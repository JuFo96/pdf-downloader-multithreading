        function slugify(text) {
            return text.toString().toLowerCase()
                .replace(/\s+/g, '-')            
                .replace(/[áàäâ]/g, 'a')         
                .replace(/[éèëê]/g, 'e')         
                .replace(/[íìïî]/g, 'i')         
                .replace(/[óòöô]/g, 'o')         
                .replace(/[úùüû]/g, 'u')         
                .replace(/[ñ]/g, 'n')            
                .replace(/[^\w\-]+/g, '')        
                .replace(/\-\-+/g, '-')          
                .replace(/^-+/, '')              
                .replace(/-+$/, '');             
          
        }
        
